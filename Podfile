
# Cocoapod
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'

inhibit_all_warnings!
use_frameworks!


def all_pods
	#pod "CRPermissions", :path => "../"
end

def app_pods
	pod 'FontAwesomeKit'
end


target 'CRPermissions' do
	all_pods
	app_pods
end

target 'CRPermissionsTests' do
	all_pods
end

target 'CRPermissionsUITests' do
	all_pods
end